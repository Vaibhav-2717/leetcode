class Solution {
    public List<Integer> inorderTraversal(TreeNode root) {
        List<Integer> l = new ArrayList<>();
        if(root==null) return l;
        inorder(root,l);
       return l; 
    }
    public void inorder(TreeNode root,List<Integer>l){
        if(root!=null){
            if(root.left!=null){
        inorder(root.left,l);
            }
        l.add(root.val);
        if(root.right!=null){
            inorder(root.right,l);
        }
    }
    }
}
